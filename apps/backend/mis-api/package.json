{
  "name": "@repo/mis-api",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "start": "node dist/server/index.js",
    "start:docker": "node scripts/start-docker.js",
    "dev": "node scripts/nuke-and-init-db.js && tsx watch src/index.ts",
    "dev:tsc": "tsc -p tsconfig.build.json --watch",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noemit",
    "lint": "eslint src/",
    "test": "LOG_LEVEL=silent vitest"
  },
  "exports": {
    ".": {
      "types": "./dist/src/hc.d.ts",
      "default": "./dist/src/hc.js"
    }
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.2.0",
    "@hono/node-server": "^1.13.8",
    "@hono/zod-openapi": "^0.16.4",
    "@scalar/hono-api-reference": "^0.5.173",
    "@web-std/file": "^3.0.3",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "drizzle-orm": "^0.40.0",
    "drizzle-zod": "^0.7.0",
    "hono": "^4.6.20",
    "hono-pino": "^0.4.0",
    "hono-sessions": "^0.7.1",
    "pg": "^8.13.1",
    "pino": "^9.5.0",
    "pino-pretty": "^11.3.0",
    "stoker": "^1.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@repo/eslint-config": "*",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.11.17",
    "@types/pg": "^8.11.10",
    "drizzle-kit": "^0.30.5",
    "eslint": "^9.12.0",
    "eslint-plugin-format": "^0.1.2",
    "tsx": "^4.19.2",
    "vitest": "^2.1.5"
  }
}
